<aside class="left-sidebar">
    <!-- Sidebar scroll-->
    <div>
      <div class="brand-logo d-flex align-items-center justify-content-between">
        <a href="./index.html" class="text-nowrap logo-img">
          <img src="/images/logo.png" width="180" alt="" />
        </a>
        <div class="close-btn d-xl-none d-block sidebartoggler cursor-pointer" id="sidebarCollapse">
          <i class="ti ti-x fs-8"></i>
        </div>
      </div>
      <!-- Sidebar navigation-->
      <nav class="sidebar-nav scroll-sidebar" data-simplebar="">
        <ul id="sidebarnav">
            <% menus.forEach(function(menu , index) { %>
              <% if (menu.subMenues) { %>
                <li class="sidebar-item">
                  <a class="sidebar-link" href=<%= '#'+index %> data-bs-toggle="collapse" aria-expanded="false">
                    <span>
                      <i class="<%= menu.icon %>" ></i>
                    </span>
                    <span class="hide-menu"><%= menu.text %></span>
                  </a>
    
    
                  <ul class="collapse nav flex-column ms-1" id=<%= index %> data-bs-parent="#menu">
                    <% menu.subMenues.forEach(function(subMenu) { %>
                      <li class="sidebar-item">
                        <a class="sidebar-link" href="#" aria-expanded="false">
                          <span>
                            <i class="ti ti-control-play" ></i>
                          </span>
                          <span class="hide-menu"><%= subMenu %></span>
                        </a>
                      </li>
                  <% }); %>
                  </ul>

              </li>
              <% } else { %>
                <li class="sidebar-item">
                  <a class="sidebar-link" href=<%= menu.link %> aria-expanded="false">
                    <span>
                      <i class="<%= menu.icon %>" ></i>
                    </span>
                    <span class="hide-menu"><%= menu.text %></span>
                  </a>
                </li>
              <% } %>
          <% }); %>
        </ul>
        <div class="unlimited-access hide-menu bg-light-primary position-relative mb-7 mt-5 rounded">
          <div class="d-flex">
            <div class="unlimited-access-title me-3">
              <h6 class="fw-semibold fs-4 mb-6 text-dark w-85">Upgrade to pro</h6>
              <a href="https://adminmart.com/product/modernize-bootstrap-5-admin-template/" target="_blank" class="btn btn-primary fs-2 fw-semibold lh-sm">Buy Pro</a>
            </div>
            <div class="unlimited-access-img">
              <img src="/dashboard/images/backgrounds/rocket.png" alt="" class="img-fluid">
            </div>
          </div>
        </div>
      </nav>
      <!-- End Sidebar navigation -->
    </div>
    <!-- End Sidebar scroll-->
  </aside>